git add file //stage the file
git status -s //check the status
git reset HEAD file //unstage the file
git checkout file //rollback the file
git commit 



git remote -v
git remote add ssh_repo ssh:.....git

git remote remove ssh_repo

git checkout -b test v1.1

git config --global alias.zhuangtai status
git config --global alias.st status

git tag -a v1.1.1 -m "lightweight tag list"
git tag -a v1.2 -m "test tag" hash-commit

git remote show
git push origin v1.1
git push origin --tags

git branch MyTest //Create one branch based on the current branch and named MyTest
git checkout MyTest
git checkout -b MyTest 

git log --graph --oneline --all //show log in all branches

git merge MyTest //incorporate the MyTest into the current branch
git mergetool	//solve the conflict
git merge 
git branch -d MyTest //delete MyTest branch

git push origin --delete MyTest

git branch --merged	//the branches have been merged into the current branch
git branch --no-merged

git checkout MyTest
git rebase master
git checkout master
git merge MyTest

git checkout master
git merge MyTest

